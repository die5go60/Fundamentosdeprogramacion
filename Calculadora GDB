#include <iostream> //Librerias

//Utiliza la salida y la entrada estandar teclado y monitor
using namespace std;

int main()
{
    //Declaro variables en C++
   int A,B,C;
   cout<<"Ingresa un valor: ";
   cin>>A;
   cout<<"Ingresar el segundo valor: ";
   cin>>B;
   C=A+B;
   cout<<"La suma es: " <<C<<endl;
   C=A-B;
   cout<<"La resta es: "<<C<<endl;
   C=A*B;
   cout<<"La multiplicacion es: "<<C<<endl;
   C=A/B;
   cout<<"La division es: "<<C<<endl;
    return 0;
   
}
